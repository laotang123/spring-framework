<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="apple" class="pers.ljf.spring.debug.methodoverrides.lookup.Apple"/>
	<bean id="orange" class="pers.ljf.spring.debug.methodoverrides.lookup.Orange"/>

	<bean id="fruitPlate1" class="pers.ljf.spring.debug.methodoverrides.lookup.FruitPlate">
		<lookup-method name="getFruit" bean="apple"/>
	</bean>

	<bean id="fruitPlate2" class="pers.ljf.spring.debug.methodoverrides.lookup.FruitPlate">
		<lookup-method name="getFruit" bean="apple"/>
	</bean>
</beans>